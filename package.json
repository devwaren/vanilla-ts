    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "xss": "^1.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "tsup": "^8.5.0",
    "typescript": "^5.3.0"
