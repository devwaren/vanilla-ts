    "jwt-decode": "^4.0.0",
    "lodash-es": "^4.17.21",
    "vite": "^7.1.5",
    "xss": "^1.0.15",
    "zustand": "^5.0.7"
  },
  "devDependencies": {
    "@types/js-cookie": "^3.0.6",
    "@types/lodash-es": "^4.17.12",
    "@types/node": "^24.3.0",
    "animate.css": "^4.1.1",
